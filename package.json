  "private": true,
  "homepage": ".",
  "dependencies": {
    "@emotion/core": "^10.0.21",
    "@emotion/styled": "^10.0.17",
    "matchmedia-polyfill": "^0.3.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "smoothscroll-polyfill": "^0.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@storybook/addon-actions": "^5.2.3",
    "@storybook/addon-links": "^5.2.3",
    "@storybook/addons": "^5.2.3",
    "@storybook/react": "^5.2.3",
    "@testing-library/react": "^9.3.0",
    "@types/jest": "^24.0.18",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
    "@types/smoothscroll-polyfill": "^0.3.1",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "babel-loader": "^8.0.6",
    "eslint-config-prettier": "^6.4.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.9",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "react-scripts": "^3.2.0",
    "sw-precache": "^5.2.1",
    "typescript": "^3.6.4"
  },
  "scripts": {
    "start": "EXTEND_ESLINT=true react-scripts start",
    "build": "EXTEND_ESLINT=true react-scripts build",
    "postbuild": "npm run optimize",
    "test": "react-scripts test --env=jsdom --testMatch **/**/test.{ts,tsx}",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "eject": "react-scripts eject",
    "optimize": "node ./node_modules/sw-precache/cli.js --root='build/' --static-file-globs='build/**/!(*map*|*CNAME*)'",
    "deploy": "npm run build && GIT_DEPLOY_DIR=build ./deploy.sh",
    "ci": "CI=true run-s lint test build",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run ci"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
